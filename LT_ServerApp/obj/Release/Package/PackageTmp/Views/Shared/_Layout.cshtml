<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - Test Bench Management</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <script src="~/Scripts/jquery-3.5.1.js"></script>
    <script src="~/Scripts/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/dataTables.bootstrap.min.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>

    <script src="~/Scripts/Role/Role.js"></script>
    <script src="~/Scripts/Users/User.js"></script>
    <script src="~/Scripts/TestBench/Priority.js"></script>
    <script src="~/Scripts/TestBench/TestBench.js"></script>
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/dataTables.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />

    
    <script>
        $(document).ready(function () {
            $('#example').DataTable({
                "filter": false,
                "info": false,
                "lengthMenu": [[5, 10], [5, 10]]
            });

            

            

           
            
        });
    </script>

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Test Bench Management", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Dashboard", "Index", "Home", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("Users", "Users", "User", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("Test Bench Configuration", "TestBench", "TestBench", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("Restore", "Restore", "Restore", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("Reports", "Index", "Help", new { area = "HelpPage" }, null)</li>
                    <li>@Html.ActionLink("Settings", "Index", "Home", new { area = "" }, null)</li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        @RenderSection("SPAViews", required: false)
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Utkarsha Enterprises</p>
        </footer>
    </div>

    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("Scripts", required: false)

</body>
</html>
